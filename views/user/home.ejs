function initNewArrivalsCarousel() {
    let $carousel = $('.new-arrivals-product-activation');

    if ($carousel.width() < 600) {
        // Wait until container gets proper width
        setTimeout(initNewArrivalsCarousel, 200);
        return;
    }

    $carousel.slick({
        slidesToShow: 4,
        slidesToScroll: 1,
        autoplay: true,
        autoplaySpeed: 3000,
        dots: false,
        arrows: true,
        lazyLoad: 'progressive',
        responsive: [
            { breakpoint: 1200, settings: { slidesToShow: 3 } },
            { breakpoint: 992,  settings: { slidesToShow: 3 } },
            { breakpoint: 768,  settings: { slidesToShow: 2 } },
