<!-- JS for Auto Slide -->
<script>
  const images = <%- JSON.stringify(handloomImages) %>;
  let currentIndex = 0;

  function showSlide(index) {
    const slider = document.getElementById("handloomSlider");
    if (slider) {
      slider.style.backgroundImage = "url('" + images[index] + "')";
    }
  }

  function changeSlide(direction) {
    currentIndex = (currentIndex + direction + images.length) % images.length;
    showSlide(currentIndex);
  }

  setInterval(() => {
    changeSlide(1);
  }, 10000); // 10 seconds
</script>

